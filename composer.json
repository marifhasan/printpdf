{
    "name": "marifhasan/printpdf",
    "description": "Quick pdf builder, Laravel, PHP",
    "type": "library",
    "homepage": "https://github.com/marifhasan/printpdf",
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0",
        "illuminate/support": "^11.0",
        "mpdf/mpdf": "^8.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Marifhasan\\PrintPDF\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Arif Hasan",
            "email": "arif.saiket@gmail.com"
        }
    ],
    "minimum-stability": "stable",
	"extra": {
		"branch-alias": {
			"dev-main": "3.1-dev"
		},
        "laravel": {
            "providers": [
                "Marifhasan\\PrintPDF\\PrintPDFServiceProvider"
            ],
            "aliases": {
                "PrintPDF": "Marifhasan\\PrintPDF\\Facades\\PrintPDF"
            }
        }
    }

}
